    <view class="node_data">
      <view class="title litText">
        <text class="titText">FYC-MTD</text>
      </view>
      <view class="data">
        <text class="bigText">当月累计FYC</text>
      </view>
       <view class="num">
        <text class="redText litText">{{fyc}}</text>
        </view>
    </view>
    <view class="node_data">
      <view class="title litText">
        <text class="titText">件数-MTD</text>
      </view>
      <view class="data">
        <text class="bigText">当月累计件数</text>
      </view>
       <view class="num">
        <text class="redText litText">{{countno}}</text>
        </view>
    </view>
  </view>
</view>


<view class="container">
<view class='title'><text>昨日业绩按产品分析</text></view>
